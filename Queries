alter table user
add map_loc_x varchar(10);

alter table user
add map_loc_y varchar(10);

alter table user
add last_donated date;

alter table user
modify last_donated int;


create table notifications (
id int primary key auto_increment,
event_name int,
user_name int,
sent varchar(5)
);

create table attendance (
id int primary key auto_increment,
event_name int,
user_name int,
reminded varchar(5)
);

alter table attendance
add reminded varchar(5);

alter table events
modify event_date int;

